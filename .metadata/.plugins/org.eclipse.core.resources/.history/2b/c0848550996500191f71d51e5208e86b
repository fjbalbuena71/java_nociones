package main;

import entidades.Persona;

public class MainPOO {
	public static void main(String[] args) {
		Persona p = new Persona();
		p.nombre = "Felix";
		p.edad = 39;
		p.peso = 82;
		
		Persona p2 = new Persona();
		p2.nombre = "Marta";
		p2.edad = 45;
		p2.peso = 67.8;
		
		p.andar (10);
		p.metros = 45;
	}

}
